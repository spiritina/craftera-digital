<section class="homepage-intro__container container" data-section="intro">
    <div class="homepage-intro">
        <div class="canvas-stars" ></div>
        <div class="homepage-intro__heading">
            <div class="homepage-intro__subtitle homepage-intro__subtitle--upper white-color" data-animation="intro">
                {{ intro.aboveTitle }}
            </div>
            <div class="homepage-intro__words" data-animation="intro">
                <img class="homepage-intro__words" src="./img/intro.svg" alt="">
            </div>
            <div class="homepage-intro__subtitle homepage-intro__subtitle--down white-color" data-animation="intro">
                {{ intro.underTitle }}
            </div>
            <div class="homepage-intro__dash"></div>
        </div>
    </div>
</section>
